# 9b02e3d6398d48b6910d2411355ba29e
#: ../usermanual/adherents.rst:41 9b02e3d6398d48b6910d2411355ba29e
msgid "send mailings,"
msgstr "inviare messaggi di posta,"

# 7e482c581aaf4376be06505df23becb4
#: ../usermanual/adherents.rst:42 7e482c581aaf4376be06505df23becb4
msgid "generate PDF labels,"
